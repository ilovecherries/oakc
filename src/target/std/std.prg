DEF PRN
 VAR N# = MACHINE_POP#()
 ?N#;
END

DEF PRS
 VAR ADDR# = MACHINE_POP#()
 VAR I%=ADDR#
 WHILE M_MEMORY#[I%]
  ?CHR$(M_MEMORY#[I%]);
  INC I%
 WEND
END

DEF PRC
 VAR N# = MACHINE_POP#()
 ?CHR$(N#);
END

DEF PREND
 ?
END

DEF GETCH
 VAR IN$=""
 REPEAT
  IN$=INKEY$()
 UNTIL IN$!=""
 ?IN$;
 MACHINE_PUSH ASC(IN$)
END